generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  userID   String  @id @db.Uuid
  email    String
  password String
  color    String
  url      String?
}

model user {
  userID   Int     @id(map: "Users_pkey")
  email    String
  password String
  color    String
  url      String?
}
